<h3>People You Owe</h3>
<table class="table table-striped">
  <tr><th>User</th><th>Amount</th></tr>
  <% _.each(owed_users, function(amount, dispName) { %>
    <tr>
      <td><%= dispName %></td>
      <td><span class="text-danger"><%= amount %></span></td>
    </tr>
  <% }) %>
</table>

<h3>People Who Owe You</h3>
<table class="table table-striped">
  <tr><th>User</th><th>Amount</th><th> </th></tr>
  <% _.each(owing_users, function(amount, dispName) { %>
    <tr>
      <td><%= dispName %></td>
      <td><%= amount %></td>
      <td>settle</td>
    </tr>
  <% }) %>
</table>